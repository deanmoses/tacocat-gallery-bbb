{{! 
	Template for the early year thumbnails, which aren't real albums
}}
<article class="thumbnail">
	<a href="{{url}}"><img src="http://tacocat.com/pix/img/{{thumbnail.url}}" width="{{thumbnail.width}}px" height="{{thumbnail.height}}px" alt="{{title}}"/></a>
	<a href="{{url}}" style="width:{{thumbnail.width}}px" class="title"><h1>{{title}}</h1></a>
</article>